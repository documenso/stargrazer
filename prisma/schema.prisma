generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GitHubStats {
  id         Int      @id @default(autoincrement())
  time       DateTime
  stars      Int      @default(0)
  forks      Int      @default(0)
  openIssues Int      @default(0)
  mergedPRs  Int      @default(0)
}
